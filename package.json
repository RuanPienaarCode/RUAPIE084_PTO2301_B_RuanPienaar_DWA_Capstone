{
  "name": "capstone-podcast",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {},
    "./dist/react": "./dist/react/index.js",
    "./dist/react/*": "./dist/react/*",
    "./dist/custom-elements.json": "./dist/custom-elements.json",
    "./dist/themes/*": "./dist/themes/*",
    "./dist/components/*": "./dist/components/*",
    "./dist/utilities/*": "./dist/utilities/*",
    "./dist/translations/*": "./dist/translations/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build.js",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --max-warnings 0 --fix",
    "preview": "vite preview",
    "start": "node scripts/build.js --serve",
    "verify": "npm run prettier:check && npm run lint && npm run build && npm run test",
    "prepublishOnly": "npm run verify",
    "prettier": "prettier --write --log-level=warn .",
    "prettier:check": "prettier --check --log-level=warn .",
    "create": "plop --plopfile scripts/plop/plopfile.js",
    "spellcheck": "cspell \"**/*.{js,ts,json,html,css,md}\" --no-progress",
    "list-outdated-dependencies": "npm-check-updates --format repo --peer",
    "update-dependencies": "npm-check-updates --peer -u && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/dom": "^1.5.3",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/styled-engine-sc": "^6.0.0-alpha.6",
    "@reduxjs/toolkit": "^1.9.1",
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.38.4",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint-config-wesbos": "^3.2.3",
    "firebase": "^10.5.2",
    "netlify": "^13.1.0",
    "react": "^17.0.0 || ^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-dom": "^17.0.0 || ^18.2.0",
    "react-icons": "^4.12.0",
    "react-material-ui-carousel": "^3.4.2",
    "react-multi-carousel": "^2.8.4",
    "react-player": "^2.13.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.5",
    "react-router-dom": "^6.18.0",
    "vite": "^4.4.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "vite": "^4.0.0"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": "vite.config.js",
  "keywords": [
    "Podcast"
  ],
  "author": "Ruan Pienaar",
  "license": "ISC"
}
